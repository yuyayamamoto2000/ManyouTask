<h1>投稿一覧</h1>
<div class="contents">
  <table>
    <tr>
      <th>タイトル</th>
      <th>内容</th>
    </tr>
    <% @tasks.each do |task| %>
    <tr>
      <td><%= task.title %></td>
      <td><%= task.content %></td>
      <td><% link_to "詳細を確認する。", task_path(task.id) %></td>
      <% end %>
    </tr>
  </table>
</div>
