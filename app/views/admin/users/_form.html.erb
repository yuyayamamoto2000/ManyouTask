<%= form_with(model: @user, local: true, url: admin_user_path ) do |f| %>
  <div class="center">
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
    <br>

    <%= f.submit '登録' %>
  </div>
<% end %>
